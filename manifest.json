{
  "manifest_version": 3,
  "name": "TEST",
  "description": "8380",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "TEST"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://outbound.coupang.com/batch/batch/get/*"]
    },
    {
      "js": ["scripts/batch.js"],
      "matches": ["https://outbound.coupang.com/order/detail/*"]
    },
    {
      "js": ["scripts/rowCopy.js"],
      "matches": ["https://outbound.coupang.com/workflow/*"]
    },
    {
      "js": ["scripts/test.js"],
      "matches": ["http://*/*", "https://*/*", "http://localhost:*/*"]
    }
  ]
}
